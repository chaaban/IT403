vech="Car"
printf "%s\n" $vech
printf "%1s\n" $vech
printf "%1.1s\n" $vech
printf "%1.2s\n" $vech
printf "%1.3s\n" $vech
printf "%10.3s\n" $vech
printf "%10.1s\n\n" $vech


big=5355765
printf "%e \n" $big
printf "%.2e \n" $big
printf "%10.2e \n\n" $big

sales=54245.22
printf "%f\n" $sales
printf "%.2f\n" $sales
printf "%.3f\n" $sales;
printf "%1.3f\n" $sales;
printf "%10.2f\n" $sales;

